{
  "name": "parikshan",
  "version": "0.0.2",
  "description": "Compiler to measure the running time of javascript functions",
  "repository": {
    "url": "https://github.com/gajananpp/parikshan"
  },
  "homepage": "https://github.com/gajananpp/parikshan/blob/main/README.md",
  "author": {
    "name": "Gajanan Patil"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "build/src/index.js",
  "bin": "build/src/cli.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "performance",
    "perf_hooks",
    "compiler",
    "transpiler",
    "timeline",
    "timerify"
  ],
  "scripts": {
    "test": "mocha build/test/*.js",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile && cpx test/assets/**/* build/test/assets",
    "posttest": "npm run lint",
    "docs": "typedoc --excludePrivate --excludeInternal --readme none src/index.ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jscodeshift": "^0.11.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.11.2",
    "@types/sinon": "^10.0.6",
    "@types/yargs": "^17.0.8",
    "cpx": "^1.5.0",
    "gts": "^3.1.0",
    "mocha": "^9.1.3",
    "sinon": "^12.0.1",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.9",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "ast-types": "^0.14.2",
    "commander": "^8.3.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "jscodeshift": "^0.13.1",
    "mongodb": "^4.2.2",
    "recast": "^0.20.5",
    "simple-git": "^2.48.0",
    "terser": "^5.10.0",
    "yargs": "^17.3.1"
  }
}
